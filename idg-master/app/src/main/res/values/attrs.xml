        <attr name="left_cell_width" format="dimension"/>
        <attr name="top_cell_height" format="dimension"/>
        <attr name="normal_cell_width" format="dimension"/>
    </declare-styleable>

    <!--自定义上下左右可以滚动的表格属性-->
